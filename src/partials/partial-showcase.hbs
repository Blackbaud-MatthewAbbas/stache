{{# withNav pattern="showcase" customData=customData }}
    {{# minify collapseWhitespace=true }}
        <div class="stache-showcase {{ containerClass }}">
            <ul class="row{{# if sameHeight }} stache-equal-height-row{{/ if }}">
                {{# each nav_links }}
                    <li class="{{ class }}">
                        {{# if ../sameHeight }}
                            {{{ include ../../template containerClass="stache-equal-height-item"}}}
                        {{ else }}
                            {{{ include ../../template }}}
                        {{/ if }}
                    </li>
                    {{ include 'partial-showcase-row-breakpoints' }}
                {{/ each }}
            </ul>
        </div>
    {{/ minify }}
{{/ withNav }}
