<!DOCTYPE html>
<html lang="en" class="{{# isHome dest=page.dest }}full-height-width{{/ isHome }}" ng-app="stache">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ stache.config.title }}{{# if title }} | {{ title }}{{/ if }}</title>
    <link rel="icon" href="{{ assets }}img/favicon.ico" type="image/ico">
      
    <!-- build:css {{ assets }}css/app.css -->
    <link rel="stylesheet" href="{{ assets }}css/app.css">
      {{# if stache.config.sky }}
      <link rel="stylesheet" href="{{ assets }}vendor/angular-toastr/dist/angular-toastr.css">
      <link rel="stylesheet" href="{{ assets }}vendor/angular-ui-select/dist/select.css">
      {{/ if }}
    <!-- endbuild -->

    {{# each stache.config.files.css }}
      <link rel="stylesheet" href="{{ this }}">
    {{/ each }}

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

  </head>
  <body class="{{# if stache.config.showOmnibar }}bb-omnibar-height-padding{{/ if }} {{# isHome dest=page.dest }}full-height-width{{/ isHome }}">

    <div id="top"></div>

    {{# if stache.config.showHeader }}
      {{# if stache.config.header }}
        {{ include stache.config.header }}
      {{ else }}

        <nav class="navbar navbar-default bb-navbar nav-wrap">
          <div class="container">
            <div class="row">
              <div class="col-sm-12">
                {{{ include stache.config.partial_header_nav }}}
                {{{ include stache.config.partial_header_search }}}
              </div>
            </div>
          </div>
        </nav>

      {{/ if }}
    {{/ if }}

    {{# if stache.config.markdown }}
      {{# markdown }}{{ body }}{{/ markdown }}
    {{ else }}
      {{# withNewline }}{{ body }}{{/ withNewline }}
    {{/ if }}

    {{# if note }}
      {{# withHash isPageNote=true priority=priority note=note }}
        {{> partial-note }}
      {{/ withHash }}
    {{/ if }}

    {{# if stache.config.showFooter }}
      {{# if stache.config.footer }}
        {{ include stache.config.footer }}
      {{ else }}

        <div class="footer-site">
          <div class="container">
            <div class="row">
              <div class="col-xs-6 col-sm-4">
                {{{ include stache.config.partial_footer_logo }}}
                {{{ include stache.config.partial_footer_navigation }}}
              </div>
              <div class="col-xs-6 col-sm-4">
                {{{ include stache.config.partial_footer_internal }}}
              </div>
              <div class="col-sm-4">
                {{{ include stache.config.partial_footer_social }}}
              </div>
            </div>

          </div>
        </div>

      {{/ if }}
    {{/ if }}

    <div class="copyright">
      <div class="container">
        <div class="row">

          <div class="col-sm-6">
            <p>&copy; Blackbaud, Inc.  All rights reserved.</p>
          </div>  <!-- .col-sm-6 -->

          <div class="col-sm-6">
          </div>  <!-- .col-sm-6 -->

        </div>  <!-- .row -->
      </div>  <!-- .container -->
    </div>  <!-- .copyright -->

    <!-- Omnibar Placeholder -->
    <div id="omnibar"></div>

    <!-- build:js {{ assets }}js/lib.js -->
    <script src="{{ assets }}js/libs/easyXDM.min.js"></script>
    <script src="{{ assets }}vendor/jquery/dist/jquery.min.js"></script>
    <script src="{{ assets }}vendor/angular/angular.min.js"></script>
    <script src="{{ assets }}vendor/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="{{ assets }}vendor/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script src="{{ assets }}vendor/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="{{ assets }}vendor/holderjs/holder.min.js"></script>
    <script src="{{ assets }}vendor/moment/moment.js"></script>
    <script src="{{ assets }}vendor/stellar/jquery.stellar.js"></script>
    <!-- endbuild -->

    {{# if stache.config.sky }}
      <!-- build:js {{ assets }}js/sky.js -->
      <script src="{{ assets }}vendor/angular-animate/angular-animate.js"></script>
      <script src="{{ assets }}vendor/angular-toastr/dist/angular-toastr.tpls.js"></script>
      <script src="{{ assets }}vendor/angular-ui-bootstrap-bower/ui-bootstrap-tpls.js"></script>
      <script src="{{ assets }}vendor/angular-ui-select/dist/select.js"></script>
      <script src="{{ assets }}vendor/autoNumeric/autoNumeric.js"></script>
      <script src="{{ assets }}vendor/blockui/jquery.blockUI.js"></script>
      <script src="{{ assets }}vendor/enquire/dist/enquire.js"></script>
      <script src="{{ assets }}vendor/fastclick/lib/fastclick.js"></script>
      <script src="{{ assets }}vendor/jqgrid/js/jquery.jqGrid.js"></script>
      <script src="{{ assets }}vendor/jquery-icheck/icheck.js"></script>
      <script src="{{ assets }}vendor/jquery-ui/jquery-ui.js"></script>
      <script src="{{ assets }}vendor/jqueryui-touch-punch/jquery.ui.touch-punch.min.js"></script>
      <script src="{{ assets }}vendor/responsive-tabs/js/jquery.responsiveTabs.js"></script>
      <script src="{{ assets }}vendor/bootstrap-datepicker-eyecon/js/bootstrap-datepicker.js"></script>
      <script src="{{ assets }}vendor/bb-sky-sass/dist/js/sky.min.js"></script>
      <!-- endbuild -->
    {{/ if }}

    <!-- build:js {{ assets }}js/app.js -->
    <script src="{{ assets }}js/search.js"></script>
    <script src="{{ assets }}js/app.js"></script>
    <!-- endbuild -->

    {{# each stache.config.files.js }}
      <script src="{{ this }}"></script>
    {{/ each }}

    {{# each files.js }}
      <script src="{{ this }}"></script>
    {{/ each }}

    {{# if stache.config.showOmnibar }}
      <script src="//signin.blackbaud.com/omnibar.min.js"></script>
      {{# if stache.config.omnibar }}
        {{ include stache.config.omnibar }}
      {{ else }}
        <script>
        {{{ include stache.config.partial_omnibar }}}
        </script>
      {{/ if }}
    {{/ if }}

  </body>
</html>
