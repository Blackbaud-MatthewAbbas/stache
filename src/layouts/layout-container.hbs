---
layout: layout-base
---

<div class="content">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        {{# inherit stache.config.markdown markdown }}
          {{# markdown }}{{ body }}{{/ markdown }}
        {{ else }}
          {{# withNewline }}{{ body }}{{/ withNewline }}
        {{/ inherit }}
      </div>
    </div>
  </div>
</div>
